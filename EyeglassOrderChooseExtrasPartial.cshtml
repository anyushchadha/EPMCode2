<div class="form-horizontal col-lg-12 col-md-12 col-md-no-padding-left">
    <div id="egOrderExtras">
        <div id="egExtrasMsgPanel" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 col-no-padding">
            <div id="msgError" class="alert alert-danger messageCenterStyle hidden" data-selectable="false">
                <i class="icon-notification"></i><span id="errorMsgTitle" class="message"></span>
            </div>
        </div>

        <div id="firstExtrasGroup">
            <h3 id="title1" class="extras-title">VSP Eligible Extras</h3>
            <div class="row" data-bind="foreach: {data: displayEgOrderExtrasVsp, as: 'egRow1'}">
                <div class="" data-bind="foreach: $data">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 extras-panel panelLink" data-bind="attr: { id: 'orderExtras_' + Id }">
                            <i class="icon-checkmark-circle color-13 hidden" data-bind="attr: { id: 'extraId_' + Id }"></i>
                            <div class="text-center form-group">
                                <h5 data-bind="text: Name"></h5>
                            </div>
                            <div class="text-center">
                                <img style="width: 38px; height: 28px" data-bind="attr: { id: 'extraLogoId_' + Id }" src="~/Content/themes/app/images/VSP_RGB_56x47px.png"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="secondExtrasGroup">
            <h3 id="title2">VSP Ineligible Extras</h3>
            <div class="row" data-bind="foreach: {data: displayEgOrderExtrasNonVsp, as: 'egRow2'}">
                <div class="" data-bind="foreach: $data">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 extras-panel panelLink" data-bind="attr: { id: 'orderExtras_' + Id }">
                            <i class="icon-checkmark-circle color-13 hidden" data-bind="attr: { id: 'extraId_' + Id }"></i>
                            <div class="text-center">
                                <h5 data-bind="text: Name"></h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="allExtrasGroup">
            <div class="row" data-bind="foreach: {data: displayEgOrderExtrasAll, as: 'egRow3'}">
                <div class="" data-bind="foreach: $data">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 extras-panel panelLink" data-bind="attr: { id: 'allExtras_' + Id }">
                            <i class="icon-checkmark-circle color-13 hidden" data-bind="attr: { id: 'allExtraId_' + Id }"></i>
                            <div class="text-center">
                                <h5 data-bind="text: Name"></h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
</div>
