<form id="clOrderSummaryForm" class="form-horizontal col-lg-10 col-md-12 col-sm-12">

    <!-- Shipping Information -->
    <div id="shippingPanelContainer" class="col-lg-6 col-md-6 col-sm-12">
        <div id="shippingInfoPanel" class="panel">
            <div class="panel-heading">Shipping Information</div>
            <div class="panel-body">
                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <span class="required">*</span>
                        <label class="fieldLabel">Ship To</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <select name="shipTo" id="shipTo" data-bind="options: shipToList, value: shipTo, optionsText: 'Description', optionsValue: 'Key', css: { requiredField: shipTo() <= 0 }"></select>
                    </div>
                </div>
                <div class="col-lg-offset-3 fieldMessages"></div>
                <div class="clearfix"></div>

                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <span class="required">*</span>
                        <label class="fieldLabel">Address</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <select name="address" id="address" data-bind="options: addressList, value: address, optionsText: 'Description', optionsValue: 'Key', css: { requiredField: address() <= 0 }"></select>
                    </div>
                </div>
                <div class="col-lg-offset-3 fieldMessages"></div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>

    <!-- Order Information -->
    <div class="col-lg-6 col-md-6 col-sm-12" id="orderInformationPanelContainer">
        <div id="orderInfoPanel" class="panel">
            <div class="panel-heading">Order Information</div>
            <div class="panel-body">
                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel">Order Type</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9 form-control-static">
                        <span id="orderType" data-bind="text:orderType"></span>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <span class="required">*</span><label class="fieldLabel">Resource</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <select name="staff" id="staff" data-bind="options: staffList, value: staff, optionsText: 'Description', optionsValue: 'Key', css: { requiredField: staff() <= 0 }" data-live-search="true" data-size="10" data-dropup-auto="false"></select>
                    </div>
                </div>
                <div class="col-lg-offset-3 fieldMessages"></div>
                <div class="clearfix"></div>

                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel">Doctor</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9 form-control-static">
                        <span id="doctor" data-bind="text:doctor"></span>
                    </div>
                </div>

                <div class="form-group hidden" data-bind="css:{'hidden': !remake()}">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel">Remake Reasons</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <select name="remakeReasons" id="remakeReasons" data-bind="options: remakeReasons, value: remakeTypeId, optionsText: 'Description', optionsValue: 'Key'" data-live-search="true" data-size="10" data-dropup-auto="false"></select>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel">Parts To Supply</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <label class="checkbox-inline">
                            <input type="checkbox" id="rightLens" value="option1" data-bind="checked:toSupplyRightLens"> Right Lens
                        </label>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="leftLens" value="option1" data-bind="checked:toSupplyLeftLens"> Left Lens
                        </label>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <span class="required">*</span><label class="fieldLabel">Supplied By</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <select name="suppliedBy" id="suppliedBy" data-bind="options: suppliedByList, value: suppliedBy, optionsText: 'Description', optionsValue: 'Key', css: { requiredField: suppliedBy() <= 0 }"></select>
                    </div>
                </div>
                <div class="col-lg-offset-3 fieldMessages"></div>
                <div class="clearfix"></div>

                <div id="instructionsGroupFields" class="form-group hidden">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel">Instructions</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <textarea name="instructions" id="instructions" class="form-control" rows="3" maxlength="200" data-bind="value:labInstructions"></textarea>
                    </div>
                </div>
                <div id="instructionsGroupAdd" class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel"></label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <a href="" id="linkAddInstructions">Add Instructions</a>
                    </div>
                </div>
                <div class="col-lg-offset-3 fieldMessages"></div>
                <div class="clearfix"></div>

                <div class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label fieldLabelWrap">
                        <label class="fieldLabel"><span class="required">*</span>Dispensing <br />Status</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <select name="dispensingStatus" id="dispensingStatus" data-bind="options: dispenseTypeList, value: dispenseType, optionsText: 'Description', optionsValue: 'Key', css: { requiredField: dispenseType() <= 0 }"></select>
                    </div>
                </div>
                <div class="col-lg-offset-3 fieldMessages"></div>
                <div class="clearfix"></div>

                <div id="dispensingNotesGroupFields" class="form-group hidden">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel">Dispensing Note</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <textarea name="dispenseNote" id="dispenseNote" class="form-control" rows="3" maxlength="200" data-bind="value:dispenseNote"></textarea>
                    </div>
                </div>
                <div id="dispensingNotesGroupAdd" class="form-group">
                    <div class="col-lg-3 col-md-3 col-sm-3 control-label">
                        <label class="fieldLabel"></label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9">
                        <a href="" id="linkAddDispensingNotes">Add Dispensing Notes</a>
                    </div>
                </div>
                <div class="col-lg-offset-3 fieldMessages"></div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>

    <!-- Contact Lenses -->
    <div class="col-lg-6 col-md-6 col-sm-12" id="contactLensesPanelContainer">

        <!-- Soft Contact Lens -->
