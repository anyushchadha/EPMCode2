<form id="chooseClRxForm" class="form-horizontal col-lg-10 col-md-12">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <h4>Choose an Rx</h4>
    </div>
    <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12">
        <div id="msg_IncompleteClRxs" class="alert alert-warning messageCenterStyle hidden" data-selectable="true" data-url-type="external" data-url="">
            <span class="title"><i class="icon-warning"></i><strong>The system has detected one or more incomplete Rx's.</strong></span>
            <span class="message">
                If an Rx is not shown below, <u><strong>Click Here</strong></u> to go to the Rx screen and fix the issue.
            </span>
        </div>
        <table id="rxTable"></table>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
        @Html.Partial("~/Views/Patient/InsurancePanelPartial.cshtml")
    </div>
</form>
<div id="clPricing" class="modal fade">
    <div class="modal-dialog">
        <div class="vertical-align-middle">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="clPricingTitle">Contact Lens Price</h4>
                </div>
                <div class="modal-body">
                    <form id="saveClPriceRight" name="saveClPriceRight" method="POST" action="#" class="form-horizontal">
                        <input id="antiForgeryToken" type="hidden" value="" />
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 summaryMessages"></div>
                        <div id="rightCLPricingPanel" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 hidden">
                            <div class="form-group">
                                <div class="col-lg-4 col-md-4 col-sm-4 control-label">
                                    <label for="clNameRight">Right Contact Lens</label>
                                </div>
                                <div class="col-lg-8 col-md-8 col-sm-8 form-control-static">
                                    <span id="clNameRight"></span>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-lg-4 col-md-4 col-sm-4 control-label">
                                    <label for="retailPriceRight">Retail Price</label>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <input type="text" name="retailPriceRight" id="retailPriceRight" class="form-control " />
                                </div>
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-8 col-lg-offset-4 col-md-offset-4 col-sm-offset-4 fieldMessages"></div>
                        </div>
                    </form>
                    <br/>
                    <form id="saveClPriceLeft" name="saveClPriceLeft" method="POST" action="#" class="form-horizontal">
                        <div id="leftCLPricingPanel" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 hidden border-top">
                            <div class="form-group">
                                <div class="col-lg-4 col-md-4 col-sm-4 control-label">
                                    <label for="clNameLeft">Left Contact Lens</label>
                                </div>
                                <div class="col-lg-8 col-md-8 col-sm-8 form-control-static">
                                    <span id="clNameLeft"></span>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-lg-4 col-md-4 col-sm-4 control-label">
                                    <label for="retailPriceLeft">Retail Price</label>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2">
                                    <input type="text" name="retailPriceLeft" id="retailPriceLeft" class="form-control " />
                                </div>
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-8 col-lg-offset-4 col-md-offset-4 col-sm-offset-4 fieldMessages"></div>
                        </div>
                    </form>
                </div>
                <div class="clearfix"></div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left" id="btnClose" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" id="btnSavePrice">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>